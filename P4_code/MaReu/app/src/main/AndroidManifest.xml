<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.delombaertdamien.mareu">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_mareu"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".controller.Activity.DetailActivity"/>

        <activity
            android:name=".controller.Activity.ConfigureMeetingActivity"
            android:label="@string/title_activity_configure_meeting" />
        <activity android:name=".controller.Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>